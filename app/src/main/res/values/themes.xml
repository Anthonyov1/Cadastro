<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Cadastro" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Cadastro.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Cadastro.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Cadastro.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="inserir" parent="Theme.Cadastro">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#000</item>
        <item name="android:textColorHint">#000</item>
        <item name="android:background">#D3D3D3</item>
    </style>

    <style name="labels" parent="Theme.Cadastro">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#000</item>
        <item name="android:background">#8b4513</item>
        <item name="android:textAlignment">center</item>
    </style>

</resources>